clc;clear;
m = 2;
n = 25;
Amax = 0; Bmax = 0; check = -Inf;
for i = 1:10
    createRandModel(n, m);
    [A, B, p] = readModel('model.txt',n,m);
    test = dlmread('testdata.txt');
    obsv_prob(test, A, B, p, n)
    viterbi(test, A, B, p, n);
    data = dlmread('data.txt');
    [Amax, Bmax, h, check] = baumwelch(data, A, B, Amax, Bmax, p, n, m, check);
    plot(h)
    hold on;
end
